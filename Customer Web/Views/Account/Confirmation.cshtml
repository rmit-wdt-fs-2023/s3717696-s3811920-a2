@model MCBA_Web.ViewModels.DepositViewModel
@{
    ViewData["Title"] = "Transaction";
}

<h1 class="display-4">Account Number @Model.Account.AccountNumber (@Model.Account.AccountType)</h1>
<div class="row">
    <div class="col-md-4">
        <p>@Model.AccountNumber</p>
        <p>@Model.DestinationAccountNumber</p>
        <p>@Model.Amount</p>
        <p>@Model.Comment</p>
    </div>
</div>


@section Scripts {
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}


